{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Godot",
      "type": "shell",
      "command": "/Applications/Godot.app/Contents/MacOS/Godot",
      "args": ["--path", "."],
      "group": {
        "kind": "build",
        "isDefault": true
